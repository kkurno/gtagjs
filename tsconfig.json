{
  "compilerOptions": {
    "outDir": "lib",
    "target": "es5",
    "module": "CommonJS",
    "lib": ["ESNext", "DOM"],
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "noImplicitAny": true,
    "declaration": true,
    "moduleResolution": "Node"
  },
  "include": ["./src"]
}
